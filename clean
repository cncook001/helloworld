#!/bin/bash

kubectl delete \
    -f service1.yaml \
    -f service2.yaml \
	-f github.yaml \
	-f rebuild.yaml \
	-f secrets.yaml

rm -f cmds out

sed -i 's/\(^.*Fprintf.*"\)[^"]*\(".*$\)/\1Hello World!\\n\2/' helloworld.go
git add helloworld.go && git commit -s -m "clean" && git push origin master 
make
